{
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
        {
            "source": "/login",
            "destination": "/admin/login",
            "permanent": false
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ]
}